<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="mgr.entity">
	<class name="Dept" table="dept">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>
		<property name="deptid" type="string" column="deptid"></property>
		<property name="deptname" type="string" column="deptname"></property>
		<property name="deptparent" type="string" column="deptparent"></property>
		<property name="deptmgr" type="string" column="deptmgr"></property>
	</class>
</hibernate-mapping>